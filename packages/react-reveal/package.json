{
  "name": "@cenk1cenk2-presentations/react-reveal",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Presentation template using react and reveal.js.",
  "repository": "https://github.com/cenk1cenk2/react-presentations",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "scripts": {
    "prebuild": "rimraf tsconfig*.tsbuildinfo && rimraf dist/",
    "build": "tsc -P tsconfig.build.json",
    "dev:build": "yarn prebuild && tsc -p tsconfig.json",
    "dev:start": "yarn dev:build && tsc-watch --noClear",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "./*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "./*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
